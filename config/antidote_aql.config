{mode, max}.

{duration, 1}.

{concurrent, 3}.

{driver, basho_bench_driver_antidote_aql}.

{key_generator, {uniform_int, 50000}}.

{value_generator, {fixed_bin, 100}}.

{aql_actors, [{a, 'aqldev@127.0.0.1'}]}.
