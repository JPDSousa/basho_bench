%              {b, 'aql2@127.0.0.1'},
%              {c, 'aql3@127.0.0.1'}]}.


{operations, [{put, 1},{get, 1},{delete, 1}]}.

{code_paths, ["./deps/cqerl/ebin", "./deps/pooler/ebin",  "./deps/uuid/ebin",
  "./deps/semver/ebin"]}.